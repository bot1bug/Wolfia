		"rules": {
			"vue/multi-word-component-names": "off"
		},
		"parserOptions": {
			"parser": "@babel/eslint-parser"
		}
	},
	"prettier": {
		"printWidth": 120
	},
	"postcss": {
		"plugins": {
			"autoprefixer": {}
		}
