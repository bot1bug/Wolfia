		"bulma": "^0.9.3",
		"bulma-switch": "2.0.0",
		"bulma-toast": "^2.4.1",
		"bulmaswatch": "^0.8.1",
		"core-js": "^3.6.5",
		"vue": "^3.1.0",
		"vue-router": "^4.0.12",
		"vuex": "^4.0.2"
	},
	"devDependencies": {
		"@babel/core": "^7.12.16",
		"@babel/eslint-parser": "^7.12.16",
		"@vue/cli-plugin-babel": "~5.0.4",
		"@vue/cli-plugin-eslint": "~5.0.4",
		"@vue/cli-plugin-router": "~5.0.4",
		"@vue/cli-plugin-vuex": "~5.0.4",
